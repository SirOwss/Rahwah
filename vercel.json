{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install --production=false",
  "env": {
    "NODE_OPTIONS": "--max-old-space-size=4096"
  },
  "functions": {
    "api/**/*.ts": {
      "runtime": "nodejs18"
    }
  },
  "regions": ["iad1"]
} 